<body>
    <!--[if lt IE 8]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->

    <!-- Body main wrapper start -->
    <div class="wrapper shop">



        <!--shop main area are start-->
        <div class="shop-main-area ptb-70">
            <div class="container">
                <div class="row">
                    <!--shop sidebar start-->


                    <!--main-shop-product start-->
                    <div class="col-md-12 col-sm-8 col-xs-12">
                <div class="shop-wraper">
                    <div class="clearfix"></div>
                    <div class="shop-total-product-area clearfix mt-35">
                        <!-- Tab panes -->
                        <div class="tab-content">
                            <!--tab grid are start-->
                            <div role="tabpanel" class="tab-pane fade in active" id="grid">
                                <div class="total-shop-product-grid">
                                    {{#if products}}
                                    {{#each products}}
                                    <div class="col-md-3">
                                        <!-- single product start-->
                                        <div class="single-product">
                                            <div class="product-img">
                                                {{!-- <div class="product-label">
                                                    <div class="new">New</div>
                                                </div> --}}

                                                <div class="single-prodcut-img product-overlay pos-rltv"
                                                    {{!--style="width: 15em; height: 25em;" --}}
                                                    style="height: 350px; width: auto;">
                                                    <a href="/singleProduct/{{this._id}}"> <img alt=""
                                                            src="/public-admin/product-images/{{this.images.[0]}}"
                                                            class="primary-image"
                                                            style="height: 100%; object-fit: contain;"> <img alt=""
                                                            src="images/product/02.jpg" class="secondary-image"> </a>
                                                </div>

                                                <div class="product-icon socile-icon-tooltip text-center">
                                                    <ul>
                                                        <li><a onclick="addToCart('{{this._id}}')"
                                                                data-tooltip="Add To Cart" class="add-cart"
                                                                data-placement="left"><i
                                                                    class="fa fa-cart-plus"></i></a></li>
                                                        <li><a onclick="addToWishlist('{{this._id}}')"
                                                                data-tooltip="Wishlist" class="w-list"><i
                                                                    class="fa fa-heart-o"></i></a></li>
                                                    </ul>
                                                </div>
                                            </div>


                                            <div class="product-text">
                                                <div class="prodcut-name"> <a
                                                        href="/singleProduct/{{this._id}}">{{this.productName}}</a>
                                                </div>
                                                <div class="prodcut-ratting-price">
                                                    <div class="prodcut-price">
                                                        <div class="new-price"> Rs.{{this.price}} </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                        <!-- single product end-->
                                    </div>
                                    {{/each}}
                                    {{else}}
                                    <h2>No items matched your result</h2>
                                    {{/if}}

                                </div>
                            </div>
                            <!--shop grid are end-->

                        </div>
                    </div>
                </div>
            </div>
                    <!--main-shop-product start-->
                </div>
            </div>
        </div>
        <!--shop main area are end-->



    </div>
    <!-- Body main wrapper end -->

    <!-- Placed js at the end of the document so the pages load faster -->


</body>