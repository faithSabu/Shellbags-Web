<link rel="stylesheet" href="https://cdn.datatables.net/1.11.3/css/jquery.dataTables.min.css">
<link rel="stylesheet" href="https://cdn.datatables.net/buttons/2.0.1/css/buttons.dataTables.min.css">


<div class="col-sm-10 wrapper">
    <div class="container">
        <!-- Page-Title -->
        <div class="page-title-box">
            <div class="row align-items-center">
                <div class="col-sm-6 ">

                </div>

            </div>
        </div>
        <!-- end row -->

        <div class="row">
            <div class="col-12">
                <div class="card m-b-30">
                    <div class="card-body">

                        <h1 class="text-dark font-weight-bold my-5"> Monthly Revenue </h1>
                        <table id="simpleMonthlyReport" class="table table-striped table-bordered dt-responsive nowrap"
                            style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                            <thead>
                                <tr>
                                    <th>Year</th>
                                    <th>Jan</th>
                                    <th>Feb</th>
                                    <th>Mar</th>
                                    <th>Apr</th>
                                    <th>May</th>
                                    <th>Jun</th>
                                    <th>Jul</th>
                                    <th>Aug</th>
                                    <th>Sept</th>
                                    <th>Oct</th>
                                    <th>Nov</th>
                                    <th>Dec</th>
                                </tr>
                            </thead>

                            <tbody>
                                <tr>
                                    <td>2022</td>
                                    {{#each monthlyRevenue2022}}
                                    {{#each this}}
                                    {{#if this.revenue}}
                                    <td>{{this.revenue}}</td>
                                    {{else}}
                                    <td>0</td>
                                    {{/if}}
                                    {{/each}}
                                    {{/each}}
                                </tr>
                                <tr>
                                    <td>2021</td>
                                    {{#each monthlyRevenue2021}}
                                    {{#each this}}
                                    {{#if this.revenue}}
                                    <td>{{this.revenue}}</td>
                                    {{else}}
                                    <td>0</td>
                                    {{/if}}
                                    {{/each}}
                                    {{/each}}
                                </tr>
                                {{!-- </tr> --}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div> <!-- end col -->
        </div> <!-- end row -->
    </div> <!-- end row -->
</div>
<!-- end container-fluid -->
</div>

<script type="text/javascript" language="javascript" src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdn.datatables.net/buttons/2.0.1/js/dataTables.buttons.min.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdn.datatables.net/buttons/2.0.1/js/buttons.html5.min.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdn.datatables.net/buttons/2.0.1/js/buttons.print.min.js"></script>

<script>
    $(document).ready(function () {
        $('#simpleMonthlyReport').DataTable({
            dom: 'Bfrtip',
            {{!-- dom: 'frtipB', --}} //TO MAKE THE BUTTONS AT THE BOTTOM
        buttons: [
        'copy', 'csv', 'excel', 'pdf', 'print'
    ]
        });
    } );
</script>