<script src="https://unpkg.com/js-image-zoom@0.7.0/js-image-zoom.js" type="application/javascript"></script>
<div class="container">
  <div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <a href="/admin/add-coupon"><label style="margin: 2.5em 0em "
            class="badge badge-success px-5 py-3 float-right">Add Coupon</label></a>
        <h4 class="card-title" style="margin-top: 2.5em;">Coupons</h4>
        </p>
        <table class="table table-hover">
          <thead>
            <tr>
              <th>Coupon code</th>
              <th>Discount (%)</th>
              <th>Start Date</th>
              <th>End Date</th>
              <th>Min Buy</th>
              <th>Max Cap</th>
              <th>Used count</th>
            </tr>
          </thead>
          {{#each coupons}}
          <tbody>
            <tr>
              <td>{{this.couponCode}}</td>
              <td>{{this.discount}}</td>
              <td>{{this.startDate}}</td>
              <td>{{this.endDate}}</td>
              <td>{{this.minBuy}}</td>
              <td>{{this.maxCap}}</td>
              <td>{{this.usedCount}}</td>
              
            </tr>
            
          </tbody>
          {{/each}}
        </table>
      </div>
    </div>
  </div>
</div>

