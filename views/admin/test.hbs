<div class="col-sm-10 wrapper">
    <div class="container">
        <!-- Page-Title -->
        <div class="page-title-box">
            <div class="row align-items-center">
                <div class="col-sm-6 ">

                </div>

            </div>
        </div>
        <!-- end row -->




        <div class="row">
            <div class="col-12">
                <div class="card m-b-30">
                    <div class="card-body">

                        <h4 class="mt-0 header-title mb-5 text-center">Revenue Generation-{{selectedYear}}</h4>



                        <h1 class="text-dark font-weight-bold my-5"> Monthly Revenue </h1>
                        <table id="testTable" class="table table-striped table-bordered dt-responsive nowrap"
                            style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                            <thead>
                                <tr>
                                    <th>Year</th>
                                    <th>Jan</th>
                                    <th>Feb</th>
                                    <th>Mar</th>
                                    <th>Apr</th>
                                    <th>May</th>
                                    <th>Jun</th>
                                    <th>Jul</th>
                                    <th>Aug</th>
                                    <th>Sept</th>
                                    <th>Oct</th>
                                    <th>Nov</th>
                                    <th>Dec</th>
                                </tr>
                            </thead>


                            <tbody>

                                <tr>
                                    <td>2022</td>
                                    {{#each monthlyRevenue2022}}
                                    {{#each this}}
                                    {{#if this.revenue}}
                                    <td>{{this.revenue}}</td>
                                    {{else}}
                                    <td>0</td>
                                    {{/if}}
                                    {{/each}}
                                    {{/each}}
                                </tr>
                                <tr>
                                    <td>2021</td>
                                    {{#each monthlyRevenue2021}}
                                    {{#each this}}
                                    {{#if this.revenue}}
                                    <td>{{this.revenue}}</td>
                                    {{else}}
                                    <td>0</td>
                                    {{/if}}
                                    {{/each}}
                                    {{/each}}
                                </tr>

                                {{!-- </tr> --}}

                            </tbody>

                        </table>

                    </div>
                </div>
            </div> <!-- end col -->
        </div> <!-- end row -->

    </div> <!-- end row -->

</div>
